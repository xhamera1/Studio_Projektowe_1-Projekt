# Example environment variables for the backend
# Copy this file to `.env` and replace placeholder values before running locally.

# Server / port
PORT=8080

# Database (production: use a PostgreSQL JDBC URL; development defaults to H2 in application-dev.properties)
DATABASE_URL=jdbc:postgresql://db-host:5432/your_database
DATABASE_USERNAME=your_db_user
DATABASE_PASSWORD=your_db_password

# CORS allowed origins (comma-separated)
CORS_ALLOWED_ORIGINS=http://localhost:5173

# JWT / security
SECURITY_JWT_SECRET_KEY=replace_with_a_strong_random_value
SECURITY_JWT_EXPIRATION_TIME=1075200000
